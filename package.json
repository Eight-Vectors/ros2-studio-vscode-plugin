{
  "name": "eight-vectors-studio",
  "displayName": "Eight Vectors Studio",
  "description": "Connect to ROS 2 systems via rosbridge, visualize topics, call services, and interact with ROS 2 nodes directly from VS Code",
  "version": "0.3.8",
  "publisher": "eightvectors",
  "icon": "media/evs-logo.png",
  "galleryBanner": {
    "color": "#1e2327",
    "theme": "dark"
  },
  "homepage": "https://github.com/Eight-Vectors/ros2-studio-vscode-plugin#readme",
  "bugs": {
    "url": "https://github.com/Eight-Vectors/ros2-studio-vscode-plugin/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Eight-Vectors/ros2-studio-vscode-plugin.git"
  },
  "resources": {
    "repository": "https://github.com/Eight-Vectors/ros2-studio-vscode-plugin"
  },
  "keywords": [
    "robotics",
    "robot",
    "visualization",
    "topics",
    "services",
    "nodes",
    "parameters",
    "debugging",
    "simulation",
    "telemetry",
    "sensor",
    "laser",
    "urdf",
    "occupancy-grid",
    "bag",
    "publisher",
    "subscriber",
    "development",
    "middleware",
    "real-time",
    "3d-visualization",
    "remote-control"
  ],
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Visualization",
    "Debuggers",
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "eight-vectors-studio.connect-bridge",
        "title": "Connect to Remote"
      },
      {
        "command": "eight-vectors-studio.force-reset",
        "title": "Force Reset Connection",
        "icon": "$(debug-restart)"
      },
      {
        "command": "eight-vectors-studio.stop-retry",
        "title": "Force Stop Connection",
        "icon": "$(stop)"
      },
      {
        "command": "eight-vectors-studio.disconnect-bridge",
        "title": "Disconnect",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "eight-vectors-studio.refresh-connections",
        "title": "Refresh connection",
        "icon": "$(refresh)"
      },
      {
        "command": "eight-vectors-studio.toggle-subscription",
        "title": "Subscribe"
      },
      {
        "command": "eight-vectors-studio.subscribe-to-topic",
        "title": "Subscribe"
      },
      {
        "command": "eight-vectors-studio.unsubscribe-from-topic",
        "title": "Unsubscribe"
      },
      {
        "command": "eight-vectors-studio.get-parameters",
        "title": "Get Parameters",
        "icon": "$(settings-gear)"
      },
      {
        "command": "eight-vectors-studio.reset-visualization-preference",
        "title": "Reset Visualization View Mode Preference"
      },
      {
        "command": "eight-vectors-studio.add-to-bag-recorder",
        "title": "Add to Bag Recorder",
        "icon": "$(record)"
      },
      {
        "command": "eight-vectors-studio.open-bag-recorder",
        "title": "Open Bag Recorder",
        "icon": "$(record)"
      },
      {
        "command": "eight-vectors-studio.open-message-inspector",
        "title": "Open Message/Service Inspector",
        "icon": "$(symbol-interface)"
      },
      {
        "command": "eight-vectors-studio.inspect-topic-message",
        "title": "Inspect Message Type",
        "icon": "$(info)"
      },
      {
        "command": "eight-vectors-studio.inspect-topic-from-tree",
        "title": "Inspect",
        "icon": "$(info)"
      },
      {
        "command": "eight-vectors-studio.inspect-service-type",
        "title": "Inspect Service Type",
        "icon": "$(info)"
      },
      {
        "command": "eight-vectors-studio.inspect-action-type",
        "title": "Inspect Action Type",
        "icon": "$(info)"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Extension Configuration",
      "properties": {
        "eight-vectors-studio.rosbridgeUrl": {
          "type": "string",
          "default": "ws://localhost:9090",
          "description": "Rosbridge WebSocket URL"
        },
        "eight-vectors-studio.remoteBagPath": {
          "type": "string",
          "default": "/home/ros/bags",
          "description": "Default directory path on the remote ROS 2 machine for saving bag files"
        },
        "eight-vectors-studio.messageThrottleRate": {
          "type": "number",
          "default": 100,
          "minimum": 0,
          "maximum": 1000,
          "description": "Minimum milliseconds between message updates (0 to disable throttling)"
        },
        "eight-vectors-studio.maxMessageBufferSize": {
          "type": "number",
          "default": 100,
          "minimum": 10,
          "maximum": 1000,
          "description": "Maximum number of messages to keep in buffer per topic"
        },
        "eight-vectors-studio.maxMessageDisplayLength": {
          "type": "number",
          "default": 2000,
          "minimum": 500,
          "maximum": 10000,
          "description": "Maximum characters to display for unknown message types"
        },
        "eight-vectors-studio.showFullDataForKnownTypes": {
          "type": "boolean",
          "default": true,
          "description": "Show full data arrays for known types like LaserScan ranges (may impact performance)"
        },
        "eight-vectors-studio.maxOutputLines": {
          "type": "number",
          "default": 500,
          "minimum": 100,
          "maximum": 2000,
          "description": "Maximum lines to keep in output channel before clearing (prevents memory issues)"
        },
        "eight-vectors-studio.maxMessagesToRetain": {
          "type": "number",
          "default": 10,
          "minimum": 1,
          "maximum": 100,
          "description": "Maximum number of messages to retain per topic (prevents memory issues for infrequent topics)"
        },
        "eight-vectors-studio.maxMemoryMB": {
          "type": "number",
          "default": 1,
          "minimum": 0.1,
          "maximum": 10,
          "description": "Maximum memory (in MB) allowed for message buffer per topic before trimming old messages"
        },
        "eight-vectors-studio.maxReconnectAttempts": {
          "type": "number",
          "default": 10,
          "minimum": 0,
          "maximum": 100,
          "description": "Maximum number of reconnection attempts before giving up (0 = infinite)"
        },
        "eight-vectors-studio.staticTopics": {
          "type": "array",
          "default": [
            "/robot_description",
            "/tf_static",
            "/map_metadata",
            "/map"
          ],
          "description": "List of topic names that are considered static (publish once or rarely)",
          "items": {
            "type": "string"
          }
        },
        "eight-vectors-studio.staticTopicPatterns": {
          "type": "array",
          "default": [
            ".*_static$",
            ".*_description$",
            ".*_metadata$"
          ],
          "description": "Regular expression patterns to match static topic names",
          "items": {
            "type": "string"
          }
        },
        "eight-vectors-studio.staticMessageTypes": {
          "type": "array",
          "default": [
            ".*Parameter.*",
            ".*Description.*"
          ],
          "description": "Regular expression patterns to match static message types",
          "items": {
            "type": "string"
          }
        },
        "eight-vectors-studio.staticTopicAutoDetectRate": {
          "type": "number",
          "default": 0.1,
          "minimum": 0.01,
          "maximum": 10,
          "description": "Message rate threshold (messages per second) below which topics are auto-detected as static"
        }
      }
    },
    "keybindings": [],
    "viewsWelcome": [
      {
        "view": "eightVectorsStudioWelcomeView",
        "contents": "Welcome to Eight Vectors Studio!\n\n[Connect](command:eight-vectors-studio.connect-bridge)",
        "when": "!eight-vectors-studio.isConnecting && !eight-vectors-studio.isReconnecting"
      },
      {
        "view": "eightVectorsStudioWelcomeView",
        "contents": "Welcome to Eight Vectors Studio!\n\nConnecting...\n\n[Force Stop](command:eight-vectors-studio.stop-retry)",
        "when": "eight-vectors-studio.isConnecting"
      },
      {
        "view": "eightVectorsStudioWelcomeView",
        "contents": "Welcome to Eight Vectors Studio!\n\nReconnecting...\n\n[Force Stop](command:eight-vectors-studio.stop-retry)",
        "when": "eight-vectors-studio.isReconnecting"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "eightVectorsStudioContainerView",
          "title": "Eight Vectors Studio",
          "icon": "media/evs-logo.png"
        }
      ]
    },
    "views": {
      "eightVectorsStudioContainerView": [
        {
          "id": "eightVectorsStudioWelcomeView",
          "name": "Eight Vectors Studio",
          "icon": "media/evs-logo.png",
          "when": "!eight-vectors-studio.isConnected"
        },
        {
          "id": "eightVectorsStudioTopicsView",
          "name": "Topics",
          "icon": "media/evs-logo.png",
          "when": "eight-vectors-studio.isConnected"
        },
        {
          "id": "eightVectorsStudioNodeListView",
          "name": "Nodes",
          "icon": "media/evs-logo.png",
          "when": "eight-vectors-studio.isConnected"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "eight-vectors-studio.disconnect-bridge",
          "when": "view == eightVectorsStudioTopicsView && eight-vectors-studio.isConnected",
          "group": "navigation@1"
        },
        {
          "command": "eight-vectors-studio.force-reset",
          "when": "view == eightVectorsStudioTopicsView && eight-vectors-studio.isConnected",
          "group": "navigation@2"
        },
        {
          "command": "eight-vectors-studio.stop-retry",
          "when": "view == eightVectorsStudioTopicsView && (eight-vectors-studio.isReconnecting || eight-vectors-studio.isConnecting)",
          "group": "navigation@3"
        },
        {
          "command": "eight-vectors-studio.refresh-connections",
          "when": "view == eightVectorsStudioTopicsView && eight-vectors-studio.isConnected",
          "group": "navigation@4"
        },
        {
          "command": "eight-vectors-studio.open-bag-recorder",
          "when": "view == eightVectorsStudioTopicsView && eight-vectors-studio.isConnected",
          "group": "navigation@5"
        },
        {
          "command": "eight-vectors-studio.open-message-inspector",
          "when": "view == eightVectorsStudioTopicsView && eight-vectors-studio.isConnected",
          "group": "navigation@6"
        },
        {
          "command": "eight-vectors-studio.stop-retry",
          "when": "view == eightVectorsStudioWelcomeView && (eight-vectors-studio.isReconnecting || eight-vectors-studio.isConnecting)",
          "group": "navigation@1"
        },
        {
          "command": "eight-vectors-studio.disconnect-bridge",
          "when": "view == eightVectorsStudioNodeListView && eight-vectors-studio.isConnected",
          "group": "navigation@1"
        },
        {
          "command": "eight-vectors-studio.force-reset",
          "when": "view == eightVectorsStudioNodeListView && eight-vectors-studio.isConnected",
          "group": "navigation@2"
        },
        {
          "command": "eight-vectors-studio.stop-retry",
          "when": "view == eightVectorsStudioNodeListView && (eight-vectors-studio.isReconnecting || eight-vectors-studio.isConnecting)",
          "group": "navigation@3"
        },
        {
          "command": "eight-vectors-studio.refresh-connections",
          "when": "view == eightVectorsStudioNodeListView && eight-vectors-studio.isConnected",
          "group": "navigation@4"
        },
        {
          "command": "eight-vectors-studio.open-bag-recorder",
          "when": "view == eightVectorsStudioNodeListView && eight-vectors-studio.isConnected",
          "group": "navigation@5"
        },
        {
          "command": "eight-vectors-studio.open-message-inspector",
          "when": "view == eightVectorsStudioNodeListView && eight-vectors-studio.isConnected",
          "group": "navigation@6"
        }
      ],
      "view/item/context": [
        {
          "command": "eight-vectors-studio.disconnect-bridge",
          "when": "view == eightVectorsStudioTopicsView && viewItem == nodeTree"
        },
        {
          "command": "eight-vectors-studio.add-to-bag-recorder",
          "when": "view == eightVectorsStudioTopicsView && viewItem == topic"
        },
        {
          "command": "eight-vectors-studio.subscribe-to-topic",
          "when": "view == eightVectorsStudioTopicsView && viewItem == topic",
          "group": "inline@1"
        },
        {
          "command": "eight-vectors-studio.unsubscribe-from-topic",
          "when": "view == eightVectorsStudioTopicsView && viewItem == topicSubscribed",
          "group": "inline@1"
        },
        {
          "command": "eight-vectors-studio.inspect-topic-from-tree",
          "when": "view == eightVectorsStudioTopicsView && viewItem == topic",
          "group": "inline@10"
        },
        {
          "command": "eight-vectors-studio.inspect-topic-from-tree",
          "when": "view == eightVectorsStudioTopicsView && viewItem == topicSubscribed",
          "group": "inline@10"
        },
        {
          "command": "eight-vectors-studio.inspect-topic-message",
          "when": "view == eightVectorsStudioTopicsView && viewItem == topic"
        },
        {
          "command": "eight-vectors-studio.inspect-service-type",
          "when": "view == eightVectorsStudioTopicsView && viewItem == service",
          "group": "inline"
        },
        {
          "command": "eight-vectors-studio.inspect-action-type",
          "when": "view == eightVectorsStudioTopicsView && viewItem == actionClient",
          "group": "inline"
        },
        {
          "command": "eight-vectors-studio.get-parameters",
          "when": "view == eightVectorsStudioNodeListView && viewItem == node"
        },
        {
          "command": "eight-vectors-studio.add-to-bag-recorder",
          "when": "view == eightVectorsStudioNodeListView && viewItem == publisher"
        },
        {
          "command": "eight-vectors-studio.add-to-bag-recorder",
          "when": "view == eightVectorsStudioNodeListView && viewItem == subscribers"
        },
        {
          "command": "eight-vectors-studio.inspect-topic-message",
          "when": "view == eightVectorsStudioNodeListView && viewItem == publisher",
          "group": "inline"
        },
        {
          "command": "eight-vectors-studio.inspect-topic-message",
          "when": "view == eightVectorsStudioNodeListView && viewItem == subscribers",
          "group": "inline"
        },
        {
          "command": "eight-vectors-studio.inspect-service-type",
          "when": "view == eightVectorsStudioNodeListView && viewItem == service",
          "group": "inline"
        },
        {
          "command": "eight-vectors-studio.inspect-action-type",
          "when": "view == eightVectorsStudioNodeListView && viewItem == actionClient",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint .",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "copy-webpack-plugin": "^13.0.0",
    "eslint": "^9.9.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "roslib": "^1.4.1"
  }
}

